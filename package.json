{
  "name": "clinrisk-js-monorepo",
  "version": "1.0.0-rc.1",
  "description": "An unofficial javascript port of the ClinRisk algorithms",
  "main": "dist/qrisk.js",
  "typings": "dist/qrisk.d.ts",
  "scripts": {
    "build": "./build.sh",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rh389/clinrisk-js.git"
  },
  "keywords": [
    "qrisk",
    "clinrisk"
  ],
  "author": "Rob Hogan <roberthogan@blueyonder.co.uk>",
  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/rh389/clinrisk-js/issues"
  },
  "homepage": "https://github.com/rh389/clinrisk-js#readme",
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "jest": "^23.0.0",
    "ts-jest": "^22.4.6",
    "typescript": "^2.8.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePathIgnorePatterns": ["<rootDir>/dist/"]
  }
}
